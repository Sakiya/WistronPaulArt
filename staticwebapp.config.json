{
    "routes": [
        {
            "route": "/*.{html,css,js,png,jpg,jpeg,gif,svg,webp,htm,mp4}",
            "serve": "/index.html",
            "statusCode": 200
        },
        {
            "route": "/video/*",
            "statusCode": 404
        },
        {
            "route": "/js/*",
            "statusCode": 404
        },
        {
            "route": "/img/*",
            "statusCode": 404
        },
        {
            "route": "/tmp/*",
            "statusCode": 403
        },
        {
            "route": "/private/*",
            "statusCode": 403
        },
        {
            "route": "/logs/*",
            "statusCode": 403
        },
        {
            "route": "/backup/*",
            "statusCode": 403
        },
        {
            "route": "/admin/*",
            "statusCode": 403
        },
        {
            "route": "/includes/*",
            "statusCode": 403
        },
        {
            "route": "/cgi-bin/*",
            "statusCode": 403
        },
        {
            "route": "/.github/*",
            "statusCode": 403
        },
        {
            "route": "/.git/*",
            "statusCode": 403
        },
        {
            "route": "/*.bak",
            "statusCode": 403
        },
        {
            "route": "/*.config",
            "statusCode": 403
        },
        {
            "route": "/*.json",
            "statusCode": 403
        },
        {
            "route": "/*.log",
            "statusCode": 403
        },
        {
            "route": "/*.md",
            "statusCode": 403
        },
        {
            "route": "/*.sql",
            "statusCode": 403
        },
        // {
        //     "route": "/js/",
        //     "redirect": "/",
        //     "statusCode": 301
        // },
        // {
        //     "route": "/img/",
        //     "redirect": "/",
        //     "statusCode": 301
        // },
        // {
        //     "route": "/css/",
        //     "redirect": "/",
        //     "statusCode": 301
        // },
        // {
        //     "route": "/webfonts/",
        //     "redirect": "/",
        //     "statusCode": 301
        // },
        {
            "route": "/*",
            "serve": "/index.html"
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*",
            "/img/*",
            "/video/*",
            "/webfonts/*",
            "/css/*",
            "/js/*"
        ]
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/404.html",
            "statusCode": 404
        },
        "403": {
            "rewrite": "/404.html",
            "statusCode": 403
        }
    },
    "globalHeaders": {
        "Access-Control-Allow-Origin": "https://wistronladiesopen.com",
        "Access-Control-Allow-Methods": "GET, POST",
        "Access-Control-Allow-Headers": "Content-Type",
        "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline' *.azurestaticapps.net *.wistronladiesopen.com *.wistron.com *.google-analytics.com *.googletagmanager.com *.google.com *.bing.com *.youtube.com *.ytimg.com https://fonts.googleapis.com *.googleapis.com *.gstatic.com *.facebook.net *.line-scdn.net *.line-apps.com; connect-src 'self' *.wistron.com *.wistronladiesopen.com *.google-analytics.com *.googletagmanager.com *.google.com *.bing.com *.youtube.com *.ytimg.com https://fonts.googleapis.com *.googleapis.com *.gstatic.com stats.g.doubleclick.net *.facebook.com *.line-scdn.net *.line-apps.com; style-src 'self' 'unsafe-inline' *.googleapis.com; img-src 'self' blob: data: *.wistron.com *.wistronladiesopen.com *.google.com.tw *.google-analytics.com *.googletagmanager.com *.google.com *.bing.com *.google.com *.youtube.com *.ytimg.com; font-src 'self' data: *.gstatic.com *.googleapis.com; object-src 'none'; base-uri 'self'; form-action 'self'; frame-ancestors 'none'; block-all-mixed-content; upgrade-insecure-requests; frame-src *.youtube.com *.youtube-nocookie.com;",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block",
        "X-Content-Type-Options": "nosniff",
        "Referrer-Policy": "strict-origin-when-cross-origin",
        "Permissions-Policy": "accelerometer=(), camera=(), geolocation=(), gyroscope=(), magnetometer=(), microphone=(), payment=(), usb=()",
        "Strict-Transport-Security": "max-age=31536000; includeSubDomains",
        "Cache-Control": "public, max-age=3600"
    },
    "platform": {
        "apiRuntime": "none"
    }
}